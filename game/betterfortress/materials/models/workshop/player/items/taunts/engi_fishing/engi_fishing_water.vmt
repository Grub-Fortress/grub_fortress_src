"VertexLitGeneric"
{
	"$basetexture"	"models\workshop\player\items\taunts\engi_fishing\engi_fishing"
	"$bumpmap"	"water\tfwater001_normal"
	"$envmap"	"env_cubemap"
	"$phong" "1"
	"$phongboost" "25"
	"$phongexponent" "25"	
	"$selfillum" "1"

	"$rimlight" "1"		
	"$rimlightexponent" "4"				
	"$rimlightboost" "1.5"

	"$angle" "0.0"
	"$translate" "[0.0 0.0]"
	"$center" "[0.0 0.0]"
	"$scale"	"0.25"
	"$cloakPassEnabled" "1"

  "Proxies"
{
	"spy_invis"
	{
	}
	"AnimatedTexture"
	{
		"animatedtexturevar" "$bumpmap"
		"animatedtextureframenumvar" "$bumpframe"
		"animatedtextureframerate" "30"
	}
	"TextureTransform"
	{
		"translateVar" "$translate"
		"rotateVar" "$angle"
		"centerVar" "$center"
		"scaleVar" "$scale"
		"resultVar" "$bumptransform"
	}
}